{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "barryvdh/laravel-dompdf": "^2.2",
        "brick/phonenumber": "^0.6.0",
        "dedoc/scramble": "^0.11.28",
        "doctrine/dbal": "^3.8",
        "giggsey/libphonenumber-for-php": "^8.13",
        "google/apiclient": "^2.16",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "laravel/framework": "^11.34",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.0",
        "league/flysystem-aws-s3-v3": "^3.8",
        "maatwebsite/excel": "^3.1",
        "propaganistas/laravel-phone": "^5.3",
        "spatie/laravel-backup": "^8.0",
        "stripe/stripe-php": "^15.7",
        "unsplash/unsplash": "^3.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/ApiResponse.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Google\\Cloud\\Samples\\CloudSQL\\": "app/Google/Cloud/Samples/CloudSQL/"
        },
    },
    "autoload-dev": {
        "files": [
            "app/Helpers/ApiResponse.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
